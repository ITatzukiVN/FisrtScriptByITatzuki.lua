-- T·∫°o Toggle ƒë·ªÉ b·∫≠t/t·∫Øt tƒÉng t·ªëc ƒë·ªô
local SpeedToggle = Tabs.Player:AddToggle("SpeedToggle", {
    Title = "TƒÉng t·ªëc ƒë·ªô",
    Default = false
})



-- T·∫°o Slider ƒë·ªÉ ƒëi·ªÅu ch·ªânh t·ªëc ƒë·ªô
local SpeedSlider = Tabs.Player:AddSlider("SpeedSlider", {
    Title = "T·ªëc ƒë·ªô ch·∫°y",
    Description = "ƒêi·ªÅu ch·ªânh t·ªëc ƒë·ªô nh√¢n v·∫≠t",
    Default = 16,
    Min = 16,
    Max = 100,
    Rounding = 1
})


-- Theo d√µi toggle v√† slider
task.spawn(function()
    while true do
        if Options.SpeedToggle.Value then
            local Players = game:GetService("Players")
            local player = Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:FindFirstChildOfClass("Humanoid")

            if humanoid then
                humanoid.WalkSpeed = Options.SpeedSlider.Value
            end
        else
            -- Reset t·ªëc ƒë·ªô n·∫øu t·∫Øt toggle
            local Players = game:GetService("Players")
            local player = Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:FindFirstChildOfClass("Humanoid")

            if humanoid and humanoid.WalkSpeed ~= 16 then
                humanoid.WalkSpeed = 16
            end
        end
        wait(0.2)
    end
end)



-- T·∫°o Toggle ƒë·ªÉ b·∫≠t/t·∫Øt ch·∫ø ƒë·ªô nh·∫£y cao
local JumpToggle = Tabs.Player:AddToggle("JumpToggle", {
    Title = "B·∫≠t/T·∫Øt Nh·∫£y Cao",
    Default = false
})

-- T·∫°o Slider ƒë·ªÉ ch·ªânh ƒë·ªô cao nh·∫£y
local JumpHeightSlider = Tabs.Player:AddSlider("JumpPowerSlider", {
    Title = "Ch·ªânh ƒê·ªô Cao Nh·∫£y",
    Description = "Ch·ªçn m·ª©c ƒë·ªô nh·∫£y cao",
    Default = 50,
    Min = 50,
    Max = 200,
    Rounding = 0,
})

-- Theo d√µi thay ƒë·ªïi toggle v√† slider
task.spawn(function()
    while true do
        local humanoid = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if Options.JumpToggle.Value then
                humanoid.UseJumpPower = true
                humanoid.JumpPower = Options.JumpPowerSlider.Value
            else
                humanoid.JumpPower = 50 -- Reset v·ªÅ m·∫∑c ƒë·ªãnh khi t·∫Øt
            end
        end
        wait(0.1)
    end
end)



-- T·∫°o Toggle ƒëi xuy√™n t∆∞·ªùng
local NoClipToggle = Tabs.Player:AddToggle("NoClipToggle", {
    Title = "ƒêi Xuy√™n T∆∞·ªùng",
    Default = false
})

-- Bi·∫øn tr·∫°ng th√°i
local noclip = false

-- Khi toggle thay ƒë·ªïi
Options.NoClipToggle:OnChanged(function(value)
    noclip = value

    -- Khi t·∫Øt, b·∫≠t l·∫°i CanCollide = true cho nh√¢n v·∫≠t
    if not noclip then
        local char = game.Players.LocalPlayer.Character
        if char then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
    end
end)

-- V√≤ng l·∫∑p xuy√™n t∆∞·ªùng
game:GetService("RunService").Stepped:Connect(function()
    if noclip then
        local char = game.Players.LocalPlayer.Character
        if char then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end
end)



-- Toggle Bay ki·ªÉu Infinite Yield
local FlyToggle = Tabs.Player:AddToggle("FlyToggle", {
    Title = "Fly",
    Default = false
})

-- Slider ƒëi·ªÅu ch·ªânh t·ªëc ƒë·ªô bay
local FlySpeedSlider = Tabs.Player:AddSlider("FlySpeedSlider", {
    Title = "Fly Speed",
    Default = 50,
    Min = 10,
    Max = 200,
    Rounding = 0
})

-- C√°c d·ªãch v·ª• c·∫ßn thi·∫øt
local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- Tr·∫°ng th√°i v√† setup
local flying = false
local flySpeed = FlySpeedSlider.Value
local BV = nil
local control = {F = 0, B = 0, L = 0, R = 0, U = 0, D = 0}

-- C·∫≠p nh·∫≠t t·ªëc ƒë·ªô khi slider thay ƒë·ªïi
FlySpeedSlider:OnChanged(function(val)
    flySpeed = val
end)

-- C·∫≠p nh·∫≠t ph√≠m ƒëi·ªÅu khi·ªÉn
UIS.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.W then control.F = 1 end
    if input.KeyCode == Enum.KeyCode.S then control.B = -1 end
    if input.KeyCode == Enum.KeyCode.A then control.L = -1 end
    if input.KeyCode == Enum.KeyCode.D then control.R = 1 end
end)

UIS.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.W then control.F = 0 end
    if input.KeyCode == Enum.KeyCode.S then control.B = 0 end
    if input.KeyCode == Enum.KeyCode.A then control.L = 0 end
    if input.KeyCode == Enum.KeyCode.D then control.R = 0 end
    if input.KeyCode == Enum.KeyCode.Space then control.U = 0 end
    if input.KeyCode == Enum.KeyCode.LeftControl then control.D = 0 end
end)

-- B·∫≠t / T·∫Øt bay
Options.FlyToggle:OnChanged(function(state)
    flying = state
    if flying then
        BV = Instance.new("BodyVelocity")
        BV.Velocity = Vector3.zero
        BV.MaxForce = Vector3.new(1e5, 1e5, 1e5)
        BV.P = 1250
        BV.Name = "FlyVelocity"
        BV.Parent = hrp
    else
        if BV then
            BV:Destroy()
            BV = nil
        end
        hrp.Velocity = Vector3.zero
    end
end)

-- V√≤ng l·∫∑p bay
RunService.RenderStepped:Connect(function()
    if flying and BV then
        local camCF = workspace.CurrentCamera.CFrame
        local move = Vector3.zero
        move += camCF.LookVector * (control.F + control.B)
        move += camCF.RightVector * (control.R + control.L)
        move += camCF.UpVector * (control.U + control.D)
        if move.Magnitude > 0 then
            BV.Velocity = move.Unit * flySpeed
        else
            BV.Velocity = Vector3.zero
        end
    end
end)



-- Tabs.Teleport ƒë√£ ƒë∆∞·ª£c t·∫°o s·∫µn
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")

-- Danh s√°ch t·ªça ƒë·ªô d·ªãch chuy·ªÉn
local teleportLocations = {
    ["S·∫£nh"] = Vector3.new(48, 5, -2059),
    ["Cafe"] = Vector3.new(622, 3, -941),
    ["S√¢n Bay Giang Son"] = Vector3.new(4697, 1, -1260),
    ["Khu S∆°n Xe"] = Vector3.new(1944, 1, -1092)
}

-- L∆∞u l·ª±a ch·ªçn ng∆∞·ªùi d√πng
local selectedLocation = nil

-- Dropdown ch·ªçn ƒë·ªãa ƒëi·ªÉm
Tabs.Teleport:AddDropdown("LocationDropdown", {
    Title = "Ch·ªçn ƒë·ªãa ƒëi·ªÉm",
    Values = table.pack(unpack((function()
        local keys = {}
        for name, _ in pairs(teleportLocations) do
            table.insert(keys, name)
        end
        return keys
    end)())),
    Multi = false,
    Default = nil
}):OnChanged(function(value)
    selectedLocation = teleportLocations[value]
end)

-- N√∫t x√°c nh·∫≠n d·ªãch chuy·ªÉn
Tabs.Teleport:AddButton({
    Title = "D·ªãch Chuy·ªÉn",
    Description = "Di chuy·ªÉn t·ªõi ƒë·ªãa ƒëi·ªÉm ƒë√£ ch·ªçn",
    Callback = function()
        if selectedLocation and player.Character then
            hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.new(selectedLocation)
            end
        else
            warn("Ch∆∞a ch·ªçn ƒë·ªãa ƒëi·ªÉm ho·∫∑c ch∆∞a c√≥ nh√¢n v·∫≠t.")
        end
    end
})


-- Addons:
-- SaveManager (Allows you to have a configuration system)
-- InterfaceManager (Allows you to have a interface managment system)

-- Hand the library over to our managers
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

-- Ignore keys that are used by ThemeManager.
-- (we dont want configs to save themes, do we?)
SaveManager:IgnoreThemeSettings()

-- You can add indexes of elements the save manager should ignore
SaveManager:SetIgnoreIndexes({})

-- use case for doing it this way:
-- a script hub could have themes in a global folder
-- and game configs in a separate folder per game
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)


Window:SelectTab(1)

Fluent:Notify({
    Title = "ITatzuki Hub",
    Content = "Script ƒê√£ T·∫£i Xong.",
    Duration = 8
})

-- You can use the SaveManager:LoadAutoloadConfig() to load a config
-- which has been marked to be one that auto loads!
SaveManager:LoadAutoloadConfig()
