local hrp = character:WaitForChild("HumanoidRootPart")

-- Thông tin nhiệm vụ (★ Đã thay đổi đường dẫn đúng như bạn yêu cầu)
local requiredToolName = "Package"
local questGiver = workspace.ChoKhachSystem.KhongXoa:WaitForChild("GetJobPart")
local completePart = workspace.ChoKhachSystem.KhongXoa:WaitForChild("TargetPart")

-- Hàm dịch chuyển
local function teleportTo(part)
    if part and hrp then
        hrp.CFrame = part.CFrame + Vector3.new(0, 1, 0)
    end
end

-- Kích hoạt ProximityPrompt
local function triggerPrompt(part)
    local prompt = part:FindFirstChildWhichIsA("ProximityPrompt", true)
    if prompt then
        fireproximityprompt(prompt)
        return true
    end
    return false
end

-- Farm liên tục không cần đợi có item
local function autoQuestFast()
    teleportTo(questGiver)
    wait(0.3)
    triggerPrompt(questGiver)
    wait(0.3)
    teleportTo(completePart)
end

-- Vòng lặp toggle
task.spawn(function()
    while true do
        if Options.Farm700Toggle.Value then
            autoQuestFast()
            wait(0.01) -- càng thấp càng nhanh
        else
            wait(0.5)
        end
    end
end)

_G.autoFarm = false
_G.autoFarmRunning = false

local Toggle = Tabs.Farm:AddToggle("MyToggle", {Title = "Auto Farm Coin", Default = false })

Toggle:OnChanged(function()
    if Options.MyToggle.Value then
        _G.autoFarm = true
        if not _G.autoFarmRunning then
            _G.autoFarmRunning = true

            task.spawn(function()
                local Players = game:GetService("Players")
                local player = Players.LocalPlayer or Players:GetPlayers()[1]
                local delayTime = 1

                -- Lấy tất cả part tên "Respawning Coin"
                local teleportParts = {}
                for _, obj in ipairs(workspace:GetDescendants()) do
                    if obj:IsA("BasePart") and obj.Name == "Respawning Coin" then
                        table.insert(teleportParts, obj)
                    end
                end

                if #teleportParts == 0 then
                    warn("Không tìm thấy part nào tên 'Respawning Coin'")
                    _G.autoFarmRunning = false
                    return
                end

                -- Bắt đầu vòng lặp farm
                while _G.autoFarm do
                    local char = player.Character or player.CharacterAdded:Wait()
                    local hrp = char:FindFirstChild("HumanoidRootPart")

                    for _, part in ipairs(teleportParts) do
                        if not _G.autoFarm then break end -- ⚠ kiểm tra dừng giữa vòng
                        if part and hrp then
                            hrp.CFrame = part.CFrame
                        end
                        wait(delayTime)
                    end
                end

                _G.autoFarmRunning = false
                print("Dừng auto farm.")
            end)
        end
    else
        _G.autoFarm = false
    end
end)



local targetItem = "Classic Sword: 200 VND" -- ← Đổi tên món bạn muốn mua

Tabs.Shop:AddButton({
    Title ="Sword",
    Description = "Tốn 200VNĐ",
    Callback = function()
        local parentFolder = workspace:FindFirstChild("NK ITEM") -- ← sửa lại tên đúng
        local count = 0

        if parentFolder then
            for _, obj in pairs(parentFolder:GetDescendants()) do
                if obj:IsA("ClickDetector") and obj.Parent and obj.Parent.Name == "Toolshop" then
                    local shop = obj.Parent
                    local surfaceGui = shop:FindFirstChildOfClass("SurfaceGui")

                    if surfaceGui then
                        for _, guiObj in ipairs(surfaceGui:GetDescendants()) do
                            if guiObj:IsA("TextLabel") or guiObj:IsA("TextButton") then
                                if string.lower(guiObj.Text):find(string.lower(targetItem)) then
                                    fireclickdetector(obj)
                                    count += 1
                                    break
                                end
                            end
                        end
                    end
                end
            end
        else
            warn("Không tìm thấy thư mục chứa toolshop.")
        end

        Fluent:Notify({
            Title = "Đã Mua Sword",
            Content = "-200VNĐ",
            Duration = 3
        })
    end
})




