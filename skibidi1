-- Tạo Toggle 500
local Farm500Toggle = Tabs.Farm:AddToggle("Farm500Toggle", {
    Title = "Farm 500",
    Default = false
})

-- Dịch vụ & người chơi
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- Thông tin nhiệm vụ
local requiredToolName = "Package"
local questGiver = workspace.ChoKhach2System.KhongXoa2:WaitForChild("GetJobPart")
local completePart = workspace.ChoKhach2System.KhongXoa2:WaitForChild("TargetPart")

-- Hàm dịch chuyển
local function teleportTo(part)
    if part and hrp then
        hrp.CFrame = part.CFrame + Vector3.new(0, 1, 0)
    end
end

-- Kích hoạt ProximityPrompt
local function triggerPrompt(part)
    local prompt = part:FindFirstChildWhichIsA("ProximityPrompt", true)
    if prompt then
        fireproximityprompt(prompt)
        return true
    end
    return false
end

-- Farm liên tục không cần đợi item
local function autoQuestFast()
    teleportTo(questGiver)
    wait(0.3)
    triggerPrompt(questGiver)
    wait(0.3)
    teleportTo(completePart)
end

-- Vòng lặp toggle
task.spawn(function()
    while true do
        if Options.Farm500Toggle.Value then
            autoQuestFast()
            wait(0.01) -- càng thấp càng nhanh
        else
            wait(0.5)
        end
    end
end)

-- Tạo Toggle 700
local Farm700Toggle = Tabs.Farm:AddToggle("Farm700Toggle", {
    Title = "Farm 700",
    Default = false
})

-- Dịch vụ & người chơi
local Players = game:GetService("Players")
